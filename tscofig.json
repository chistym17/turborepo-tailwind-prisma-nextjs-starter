{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["dom", "es2015"],
      "jsx": "react",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "outDir": "dist"
    }
  }
  